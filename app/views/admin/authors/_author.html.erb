<tr>
  <td><%= author.id %></td>
  <td><%= author.name %></td>
  <td><%= author.created_at %></td>
  <td>
    <%= link_to edit_admin_author_path(author) do %>
      <span><i class="fa fa-edit fa-1px"></i></span>
    <% end %>
  </td>
  <td>
    <%= link_to admin_author_path(author), method: :delete do %>
      <span><i class="fa fa-times fa-1px"></i></span>
    <% end %>
  </td>
</tr>